{
    "name": "@erudit-js/cog",
    "version": "3.0.0-dev.10",
    "description": "⚙️ Erudit kit, types and utils",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/erudit-js/erudit.git",
        "directory": "packages/cog"
    },
    "exports": {
        "./schema": {
            "types": "./dist/schema/index.d.ts",
            "import": "./dist/schema/index.mjs",
            "require": "./dist/schema/index.mjs"
        },
        "./kit": {
            "types": "./dist/kit/index.d.ts",
            "import": "./dist/kit/index.mjs",
            "require": "./dist/kit/index.mjs"
        },
        "./utils/*": "./dist/utils/*.mjs",
        "./types": {
            "types": "./types.d.ts"
        }
    },
    "files": [
        "dist",
        "types.d.ts"
    ],
    "license": "MIT",
    "scripts": {
        "build": "bun run prepack",
        "prepack": "bun unbuild"
    },
    "dependencies": {
        "gradient-string": "^3.0.0"
    },
    "peerDependencies": {
        "@bitran-js/core": "link:@bitran-js/core",
        "@bitran-js/renderer-vue": "link:@bitran-js/renderer-vue",
        "@bitran-js/transpiler": "link:@bitran-js/transpiler"
    },
    "devDependencies": {
        "unbuild": "^3.5.0"
    }
}
