{
    "name": "@erudit-js/bitran-elements",
    "description": "ğŸ“ A collection of Bitran elements for Erudit sites",
    "version": "3.0.0-dev.10",
    "type": "module",
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.mjs"
        },
        "./defaultTranspilers": {
            "types": "./dist/elements/defaultTranspilers.d.ts",
            "import": "./dist/elements/defaultTranspilers.mjs",
            "require": "./dist/elements/defaultTranspilers.mjs"
        },
        "./defaultRenderers": {
            "types": "./dist/elements/defaultRenderers.d.ts",
            "import": "./dist/elements/defaultRenderers.mjs",
            "require": "./dist/elements/defaultRenderers.mjs"
        },
        "./aliases/*": {
            "types": "./dist/elements/aliases/*.d.ts",
            "import": "./dist/elements/aliases/*.mjs",
            "require": "./dist/elements/aliases/*.mjs"
        },
        "./link/*": {
            "types": "./dist/elements/link/*.d.ts",
            "import": "./dist/elements/link/*.mjs",
            "require": "./dist/elements/link/*.mjs"
        },
        "./details/*": {
            "types": "./dist/elements/details/*.d.ts",
            "import": "./dist/elements/details/*.mjs",
            "require": "./dist/elements/details/*.mjs"
        },
        "./heading/*": {
            "types": "./dist/elements/heading/*.d.ts",
            "import": "./dist/elements/heading/*.mjs",
            "require": "./dist/elements/heading/*.mjs"
        },
        "./heading/Heading.vue": {
            "import": "./dist/elements/heading/Heading.vue"
        },
        "./include/*": {
            "types": "./dist/elements/include/*.d.ts",
            "import": "./dist/elements/include/*.mjs",
            "require": "./dist/elements/include/*.mjs"
        },
        "./math/*": {
            "types": "./dist/elements/math/*.d.ts",
            "import": "./dist/elements/math/*.mjs",
            "require": "./dist/elements/math/*.mjs"
        },
        "./accent/*": {
            "types": "./dist/elements/accent/*.d.ts",
            "import": "./dist/elements/accent/*.mjs",
            "require": "./dist/elements/accent/*.mjs"
        },
        "./emphasis/*": {
            "types": "./dist/elements/emphasis/*.d.ts",
            "import": "./dist/elements/emphasis/*.mjs",
            "require": "./dist/elements/emphasis/*.mjs"
        },
        "./list/*": {
            "types": "./dist/elements/list/*.d.ts",
            "import": "./dist/elements/list/*.mjs",
            "require": "./dist/elements/list/*.mjs"
        }
    },
    "typesVersions": {
        "*": {
            "aliases/*": [
                "./dist/elements/aliases/*.d.ts"
            ],
            "link/*": [
                "./dist/elements/link/*.d.ts"
            ],
            "details/*": [
                "./dist/elements/details/*.d.ts"
            ],
            "heading/*": [
                "./dist/elements/heading/*.d.ts"
            ],
            "include/*": [
                "./dist/elements/include/*.d.ts"
            ],
            "math/*": [
                "./dist/elements/math/*.d.ts"
            ],
            "accent/*": [
                "./dist/elements/accent/*.d.ts"
            ],
            "emphasis/*": [
                "./dist/elements/emphasis/*.d.ts"
            ],
            "list/*": [
                "./dist/elements/list/*.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "pretty": "prettier --write .",
        "dev": "bun run dev.ts",
        "build": "bun mkdist -d --loaders=js",
        "prepack": "bun run build"
    },
    "peerDependencies": {
        "@bitran-js/core": "link:@bitran-js/core",
        "@bitran-js/renderer-vue": "link:@bitran-js/renderer-vue",
        "@bitran-js/transpiler": "link:@bitran-js/transpiler",
        "@erudit-js/cog": "workspace:3.0.0-dev.10",
        "katex": "^0.16.21",
        "sass": ">=1.85.1",
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "mkdist": "^2.2.0",
        "vite": "^6.2.1",
        "vue-tsc": "^2.2.8"
    }
}
