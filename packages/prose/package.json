            "types": "./dist/jsx-dev-runtime.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "peerDependencies": {
        "vue": "latest"
    },
    "devDependencies": {
        "unbuild": "^3.6.1",
        "vitest": "^3.2.4"
