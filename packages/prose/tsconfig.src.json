{
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "outDir": "dist",
    "types": ["./env.d.ts"]
  },
  "include": ["src/**/*"],
  "exclude": ["src/elements/**/_global.*"]
}
